{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "head"
    ],
    "properties": {
        "head": {
            "type": "object",
            "properties": {
                "additionalProperties": false,
                "link": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "format": "uri"
                    }
                },
                "vars": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "boolean": {
            "type": "boolean"
        },
        "results": {
            "type": "object",
            "required": [
                "bindings"
            ],
            "properties": {
                "bindings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "uri",
                                        "literal",
                                        "bnode"
                                    ]
                                },
                                "value": {
                                    "type": "string"
                                },
                                "xml:lang": {
                                    "type": "string",
                                    "maxLength": 35
                                },
                                "datatype": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            },
                            "required": [
                                "type",
                                "value"
                            ]
                        }
                    }
                }
            }
        }
    }
}